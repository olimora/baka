# format casu
format.time <- function(x) UseMethod("format.time")
format.time.difftime <- function(x) {
units(x) <- "secs"
x <- unclass(x)
NextMethod()
}
ft.difftime <- cmpfun(format.time.difftime)
format.time.default <- function(x) {
y <- abs(x)
sprintf("%s%02dh:%02dm:%02ds",
ifelse(x < 0, "-", ""), # sign
y %% 86400 %/% 3600,  # hours
y %% 3600 %/% 60,  # minutes
y %% 60 %/% 1) # seconds
}
